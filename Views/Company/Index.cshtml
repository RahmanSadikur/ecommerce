@model  IEnumerable<NMS.Models.Company>







<nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
    <div class="navbar-collapse collapse w-100 dual-collapse2 order-1 order-md-0">
        <ul class="nav navbar-nav ml">
            
            @if (Model.Count() > 0)
            {
                <li class="nav-item">

                    <shadow>
                        <h5>
                            @Html.ActionLink(@Model.FirstOrDefault().companyName, "About", new { Controller = "Company", id = @Model.FirstOrDefault().companyID }, new { @class = "nav-link" })
                        </h5>
                    </shadow>


                </li>
                <li class="nav-link">

                    &nbsp; &nbsp; Contact at:<shadow> @Model.FirstOrDefault().companyPhone</shadow>

                </li>
            }
        </ul>
    </div>

    <div class="mx-auto my-2 order-0 order-md-1 position-relative zoom">
        <a class="mx-auto" href="#">
            <img src="//placehold.it/120/ccff00" class="rounded-circle">
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".dual-collapse2">
            <span class="navbar-toggler-icon"></span>
        </button>
    </div>
    <div class="navbar-collapse collapse w-100 dual-collapse2 order-2 order-mr-2 ">
        <ul class="nav navbar-nav ml-auto">

      
            <li class="nav-item">
                @*<a class="nav-link" href="#">Register</a>*@
                @Html.ActionLink("Register", "Index", new { Controller = "Register" }, new { @class = "nav-link" })
            </li>
            <li class="nav-item">
                @*<a class="nav-link" href="#">Login</a>*@
                @Html.ActionLink("Login", "Index", new { Controller = "Login" }, new { @class = "nav-link" })
            </li>
        </ul>
    </div>
</nav>








